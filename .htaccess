RewriteRule ^(.*)$ $1.php

RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}
RewriteCond %{HTTP_HOST} ^www.admin.tgshop.pro$ [NC]
